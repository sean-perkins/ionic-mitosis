{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "jsx": "preserve",
    "jsxImportSource": "@builder.io/mitosis",
    "module": "ESNext",
    "declaration": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "paths": {
      "~/helpers/*": ["./src/helpers/*"],
      "~/models/*": ["./src/models/*"],
      "~/*": ["./src/*"]
    }
  }
}
